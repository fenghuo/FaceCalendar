<p>event_count:<%= session[:current_event].length %></p>
<table>
<% if @all_event!=nil %>
	<% @all_event.each do |e| %>
		<tr>
			<td><%= e.eventname %></td>
			<td><%= e.groupname %></td>
	 		<td><%= e.desp %></td>
	 		<td><%= e.place %></td>
	 		
	 		<td><%= e.eventsid %></td>
	 		<td><%= e.weekday %></td>
			<td><%= e.starttime %></td>
			<td><%= e.endtime %></td>
			<td><%= e.eventid %></td>
		</tr>
	<% end %>

	</table>
	<table>
		<tr>
		<p>Groups:<%= session[:current_group].length %></p>
		<% session[:current_group].each do |e| %>
		
			<td>"<%= e %>"</td>
		
		<% end %>
		</tr>
		<p>Groupid</p>
		<tr>
		<% session[:current_groupid].each do |e| %>
		
			<td>"<%= e %>"</td>
		
		<% end %>
		</tr>
<% end %>
</table>
<p><%= x=DateTime.now %></p>
<p><%= y=x.change(:offset => '-900') %></p>
<p><%= z=y+(y.utc_offset-x.utc_offset)/(3600*24.0) %></p>
<p><%= z==x %></p>
<p><%= y.utc_offset %></p>
<p>test:<%= session[:test] %></p>
